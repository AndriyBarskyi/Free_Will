<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditSchedule_monday_Fragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/class_edit_1"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/schedule_edit_backgr" />

        <ImageView
            android:id="@+id/class_edit_2"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/class_edit_1"
            app:srcCompat="@drawable/schedule_edit_backgr" />

        <ImageView
            android:id="@+id/class_edit_3"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/class_edit_2"
            app:srcCompat="@drawable/schedule_edit_backgr" />

        <ImageView
            android:id="@+id/class_edit_4"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/class_edit_3"
            app:srcCompat="@drawable/schedule_edit_backgr" />

        <ImageView
            android:id="@+id/class_edit_5"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/class_edit_4"
            app:srcCompat="@drawable/schedule_edit_backgr" />

        <Button
            android:id="@+id/btn_edit_schedule"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/btn_shape"
            android:fontFamily="@font/comfortaa"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/schedule_btn"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/class_edit_5" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="@font/comfortaa"
            android:text="1"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_1"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/class_edit_1" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="@font/comfortaa"
            android:text="2"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_2"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/class_edit_2" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="@font/comfortaa"
            android:text="3"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/class_edit_3"
            app:layout_constraintTop_toTopOf="@+id/class_edit_3" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="@font/comfortaa"
            android:text="4"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_4"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_4"
            app:layout_constraintStart_toStartOf="@+id/class_edit_4"
            app:layout_constraintTop_toTopOf="@+id/class_edit_4" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="@font/comfortaa"
            android:text="5"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_5"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/class_edit_5" />

        <ImageView
            android:id="@+id/pencil_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_1"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_1"
            app:layout_constraintStart_toStartOf="@+id/class_edit_1"
            app:layout_constraintTop_toTopOf="@+id/class_edit_1"
            app:srcCompat="@drawable/pencil" />

        <ImageView
            android:id="@+id/pencil_2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_2"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_2"
            app:layout_constraintStart_toStartOf="@+id/class_edit_2"
            app:layout_constraintTop_toTopOf="@+id/class_edit_2"
            app:srcCompat="@drawable/pencil" />

        <ImageView
            android:id="@+id/pencil_3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_3"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_3"
            app:layout_constraintStart_toStartOf="@+id/class_edit_3"
            app:layout_constraintTop_toTopOf="@+id/class_edit_3"
            app:srcCompat="@drawable/pencil" />

        <ImageView
            android:id="@+id/pencil_4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_4"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_4"
            app:layout_constraintStart_toStartOf="@+id/class_edit_4"
            app:layout_constraintTop_toTopOf="@+id/class_edit_4"
            app:srcCompat="@drawable/pencil" />

        <ImageView
            android:id="@+id/pencil_5"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_5"
            app:layout_constraintEnd_toEndOf="@+id/class_edit_5"
            app:layout_constraintStart_toStartOf="@+id/class_edit_5"
            app:layout_constraintTop_toTopOf="@+id/class_edit_5"
            app:srcCompat="@drawable/pencil" />

        <EditText
            android:id="@+id/class_edit_text1"
            android:layout_width="140dp"
            android:layout_height="48dp"
            android:fontFamily="@font/comfortaa"
            android:hint="Перша пара"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_1"
            app:layout_constraintEnd_toStartOf="@+id/pencil_1"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintTop_toTopOf="@+id/class_edit_1" />

        <EditText
            android:id="@+id/class_edit_text2"
            android:layout_width="140dp"
            android:layout_height="48dp"
            android:fontFamily="@font/comfortaa"
            android:hint="Друга пара"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_2"
            app:layout_constraintEnd_toStartOf="@+id/pencil_2"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="@+id/class_edit_2" />

        <EditText
            android:id="@+id/class_edit_text3"
            android:layout_width="140dp"
            android:layout_height="48dp"
            android:fontFamily="@font/comfortaa"
            android:hint="Третя пара"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_3"
            app:layout_constraintEnd_toStartOf="@+id/pencil_3"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/class_edit_3" />

        <EditText
            android:id="@+id/class_edit_text4"
            android:layout_width="140dp"
            android:layout_height="48dp"
            android:fontFamily="@font/comfortaa"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_4"
            app:layout_constraintEnd_toStartOf="@+id/pencil_4"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toTopOf="@+id/class_edit_4" />

        <EditText
            android:id="@+id/class_edit_text5"
            android:layout_width="140dp"
            android:layout_height="48dp"
            android:fontFamily="@font/comfortaa"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/class_edit_5"
            app:layout_constraintEnd_toStartOf="@+id/pencil_5"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toTopOf="@+id/class_edit_5" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>